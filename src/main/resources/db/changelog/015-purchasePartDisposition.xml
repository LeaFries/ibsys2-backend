    <changeSet id="015-purchasePartDisposition-table" author="">
        <createSequence sequenceName="purchase_part_disposition_seq"/>
        <createTable tableName="purchase_part_disposition">
            <column name="id" type="bigint" defaultValueSequenceNext="purchase_part_disposition_seq">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="item_number" type="integer"/>
            <column name="delivery_time" type="numeric"/>
            <column name="discount_quantity" type="integer"/>
            <column name="initial_stock" type="integer" defaultValue="0"/>
            <column name="requirement_n" type="integer"/>
            <column name="requirement_n_plus_one" type="integer"/>
            <column name="requirement_n_plus_two" type="integer"/>
            <column name="requirement_n_plus_three" type="integer"/>
            <column name="order_quantity" type="integer" defaultValue="0"/>
            <column name="order_type" type="integer"/>
            <column name="order_color" type="smallint"/>
        </createTable>
    </changeSet>

