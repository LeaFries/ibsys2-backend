<?xml version="1.0" encoding="UTF-8"?>

<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
        https://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd">

    <changeSet id="010-PlannedWarehouseStock-table" author="">
        <createSequence sequenceName="PlannedWarehouseStock_id_seq"/>
        <createTable tableName="planned_warehouse_stock">
            <column name="id" type="number" defaultValueSequenceNext="PlannedWarehouseStock_id_seq">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="product" type="number" />
            <column name="amountCurrentPeriod" type="number" />
            <column name="amountNextPeriod" type="number" />
            <column name="amountPeriodThirdPeriod" type="number" />
            <column name="amountPeriodFourthPeriod" type="number" />
        </createTable>

    </changeSet>

</databaseChangeLog>